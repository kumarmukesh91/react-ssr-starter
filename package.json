{
  "name": "react-ssr-starter",
  "version": "0.0.1",
  "description": "A react StarterKit with SSR and Code Split",
  "main": "build/server/main.js",
  "scripts": {
    "watch": "webpack --mode development --watch & nodemon --watch build --exec \"node build/server/main\"",
    "start": "NODE_ENV=production node build/server/main",
    "build": "rm -rf build && webpack --mode production --progress",
    "start:dev": "node build/server/main",
    "build:dev": "rm -rf build && webpack --mode development --progress",
    "stats": "webpack --profile --json > stats.json && webpack-bundle-analyzer stats.json dist"
  },
  "author": "Kumar Mukesh",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/webpack-plugin": "^5.12.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "core-js": "^3.6.5",
    "css-loader": "^3.5.3",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "reload": "^3.0.4",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@loadable/component": "^5.12.0",
    "@loadable/server": "^5.12.0",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "clean-webpack-plugin": "^3.0.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-redux": "^7.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^3.1.0",
    "styled-components": "^5.1.0"
  }
}
